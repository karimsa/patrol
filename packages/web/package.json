{
	"name": "@karimsa/patrol-web",
	"scripts": {
		"format": "npm run pretest -- --fix",
		"pretest": "eslint --cache src/**/*.jsx",
		"start": "concurrently -k 'parcel serve src/index.html' 'parcel serve src/server.jsx'",
		"build": "NODE_ENV=production parcel build src/index.html && NODE_ENV=production IS_SERVER=true parcel build src/server.jsx --target=node --bundle-node-modules"
	},
	"devDependencies": {
		"eslint-plugin-react": "^7.18.3",
		"parcel": "^1.12.3",
		"sass": "^1.22.12"
	},
	"dependencies": {
		"@emotion/core": "^10.0.21",
		"axios": "^0.19.0",
		"bootstrap": "^4.3.1",
		"chart.js": "^2.9.2",
		"debounce": "^1.2.0",
		"jquery": "^3.4.1",
		"moment": "^2.24.0",
		"popper.js": "^1.15.0",
		"react": "^16.11.0",
		"react-dom": "^16.9.0",
		"react-redux": "^7.1.1",
		"react-router-dom": "^5.1.2",
		"redux": "^4.0.4",
		"socket.io-client": "^2.3.0"
	}
}
